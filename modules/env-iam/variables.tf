variable "project_name" {
  type        = string
  description = "Name of project"
}

variable "environment_name" {
  type        = string
  description = "Name of environment"
}

variable "app_id" {
  type        = string
  description = "Name of application"
}

variable "bucket_name" {
  type        = string
  description = "Name of application s3 bucket"
  default     = "simple-s3"
}
